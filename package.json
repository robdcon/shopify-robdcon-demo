    "mini-css-extract-plugin": "^2.8.1",
    "node-sass": "^9.0.0",
    "sass-loader": "^14.2.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "jquery-validation": "^1.20.0"
  }
